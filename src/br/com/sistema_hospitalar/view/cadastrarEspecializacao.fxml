<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" fx:id="pane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="br.com.sistema_hospitalar.controller.CadastrarEspecializacaoController">
   <children>
      <TextField fx:id="nome" layoutX="53.0" layoutY="125.0" prefHeight="25.0" prefWidth="454.0" />
      <Label layoutX="53.0" layoutY="105.0" text="Nome da Especialização:" textFill="#2323c6">
         <font>
            <Font name="Times New Roman" size="18.0" />
         </font>
      </Label>
      <ComboBox fx:id="conselhos" layoutX="53.0" layoutY="203.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="25.0" prefWidth="187.0" promptText="Selecione" />
      <Label layoutX="119.0" layoutY="30.0" text="CADASTRO DE ESPECIALIZAÇÃO" textFill="#2323c6">
         <font>
            <Font name="Times New Roman Bold Italic" size="23.0" />
         </font>
      </Label>
      <Label layoutX="48.0" layoutY="183.0" text="Conselho:" textFill="#2323c6">
         <font>
            <Font name="Times New Roman" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="valor" layoutX="342.0" layoutY="201.0" prefHeight="25.0" prefWidth="167.0" />
      <Label layoutX="342.0" layoutY="181.0" text="Valor:" textFill="#2323c6">
         <font>
            <Font name="Times New Roman" size="18.0" />
         </font>
      </Label>
      <Button fx:id="salvarBotao" alignment="CENTER" graphicTextGap="20.0" layoutX="426.0" layoutY="303.0" mnemonicParsing="false" prefHeight="45.0" prefWidth="132.0" text="Salvar" textFill="#1379a1">
         <font>
            <Font size="14.0" />
         </font>
         <effect>
            <DropShadow />
         </effect>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <graphic>
            <ImageView fitHeight="23.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../resources/icons/save.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="voltarBotao" alignment="CENTER" graphicTextGap="20.0" layoutX="267.0" layoutY="303.0" mnemonicParsing="false" prefHeight="45.0" prefWidth="132.0" text="Cancelar" textFill="#1379a1">
         <font>
            <Font size="14.0" />
         </font>
         <effect>
            <DropShadow />
         </effect>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <graphic>
            <ImageView fitHeight="23.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../resources/icons/cancel.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
